		"target": "ES5",
		"module": "commonjs",
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"removeComments": false,
		"noImplicitAny": false
	},
	"fileGlobs": [
		"*.ts",
		"typings/main.d.ts"
	],
	"exclude":[
		"typings/main",
		"typings/browser",
		"typings/browser.d.ts"
	]
}
